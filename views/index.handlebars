<div class="content1">
    {{#each articles}}
    {{#unless saved}}
    <div class="card">
        <div class="card_title">
            <a class="link" href="https://www.nature.com{{link}}" target="_blank">{{title}}</a>
            <div class="save_btn" id="{{_id}}">Save Article</div>
        </div>

        <div class="card_summary">
            {{summary}}
        </div>
    </div>
    <br />
    {{/unless}}
    {{/each}}
</div>